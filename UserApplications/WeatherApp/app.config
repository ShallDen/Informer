<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IStorageService" sendTimeout="00:25:00" />
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_WeatherService">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://localhost:8000/WeatherService.svc"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_WeatherService"
        contract="WeatherServiceReference.WeatherService" name="NetTcpBinding_WeatherService" />
      <endpoint address="http://localhost:8002/StorageService.svc" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IStorageService" contract="StorageServiceReference.IStorageService"
        name="BasicHttpBinding_IStorageService" />
    </client>
  </system.serviceModel>
  
  <appSettings>
    <add key="WeatherUpdateFromDbInterval" value="10000" />
  </appSettings>
</configuration>